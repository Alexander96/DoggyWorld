div
	h2.titleNewDog ADD NEW DOGGY
	form.form-new-dog(action="/{{userId}}/newdog/img" enctype="multipart/form-data" method="post")
		div.titleFieldDog NAME
		input.input-dog-field(type="text" ng-change="change()" placeholder="NAME" name="name" ng-model="dog.name")
		div.titleFieldDog BIRTH DATE
		input.input-dog-field(type="text" id="date-picker" placeholder="BIRTH DATE" name="age" ng-model="dog.age")
		div.titleFieldDog BREED
		input.input-dog-field(type="text" placeholder="BREED" name="breed" ng-model="dog.breed")
		div.titleFieldDog Photo
		input.input-upload-dog-photo(multiple="multiple" name="photo" type="file")
		div
			input.button-new-dog(type="submit" value="ADD DOG")